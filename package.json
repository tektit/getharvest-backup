    "@vitest/coverage-v8": "^4.0.6",
    "@vitest/ui": "^4.0.6",
    "nock": "^14.0.10",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^4.0.6"
  }
